{
  "version": "now-php@1.0.0",
  "functions": {
    "klass_backend/api/index.py": {
      "runtime": "@vercel/python@4.3.0",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "klass_backend.settings"
  },
  "buildCommand": "python manage.py collectstatic --noinput"
}
